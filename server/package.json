  "scripts": {
    "setup": "yarn --emoji --silent --ignore-scripts",
    "dev": "ts-node-dev --poll -r tsconfig-paths/register src/server",
    "seed": "prisma db push && prisma db pull && prisma generate && apidoc -i src/ -o docs/",
    "seed-migrate": "prisma migrate dev",
    "seed-delete": "prisma migrate reset",
    "lint": "eslint .",
    "format": "prettier -w .",
    "clear": "rimraf node_modules dist build"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "apidoc": {
    "name": "Din_Restaurang Docs",
    "url": "https://localhost:5000"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "express-fileupload": "^1.5.1",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "luxon": "^3.5.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/eslint": "^9.6.0",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.5.1",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.5.0",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "cors": "^2.8.5",
    "eslint": "9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "node-cron": "^3.0.3",
    "prettier": "^3.3.3",
    "prisma": "^5.18.0",
    "prisma-docs-generator": "^0.8.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": "Please use Yarn instead of NPM to install dependencies"
  }
}
