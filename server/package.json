    "cors": "2.8.5",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "54.0.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "^16.3.0",
    "node-cron": "4.2.1",
    "prettier": "3.6.2",
    "prisma": "6.14.0",
    "prisma-docs-generator": "0.8.0",
    "rimraf": "6.0.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "4.2.0",
